/**头部*/
.top_bar {
	width: 100%;
}

.top_bar_scroll {
	position: fixed;
	top: 0rpx;
	left: 0;
	z-index: 99;
	background: #fff;

	z-index: 999;
}


.box-list {
	display: flex;
	flex-direction: column;
	flex: 1; 
	overflow: hidden;
}

.box-list-scroll {
	flex: 1;
	height: 1px;
}


.top-button{ 
	position: fixed;
	bottom: 50rpx; 
	right: 30rpx; 
	opacity: .8 ;
  }

.list-scroll-view{
	display: inline-block;
	padding: 0 10rpx; 
  }